.search-top {
  display: flex;
  padding: 30rpx 40rpx;
  justify-content: space-between;
  border-bottom: 1px solid #eee;
  box-shadow: 0 1px 1px rgba(0, 0, 0, 0.06);
}
.search-top .search-left {
  width: 75%;
  padding: 10rpx;
}
.search-top .search-left .u-input--radius[data-v-113bc24f],
.search-top .search-left .u-input--square[data-v-113bc24f] {
  border-radius: 30px;
}
.search-top .search-right {
  width: 20%;
  padding: 10rpx;
}
page {
  background: #fff;
}
 .cl-card__header {
  height: 0;
}
 .cl-card {
  border-top: 1px solid #eee;
  border-bottom: 1px solid #eee;
  box-shadow: 0 1px 1px rgba(0, 0, 0, 0.06);
  border-radius: 0;
}
 .cl-button.cl-button--success {
  background-color: #FDD100;
  border-color: #FDD100;
}
.sear_record {
  width: 100%;
  height: auto;
  overflow: hidden;
  padding: 0 30rpx;
}
.sear_record .search_rTit {
  width: 90%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.sear_record .search_rTit .search_pa {
  font-size: 30rpx;
  color: #333;
  line-height: 80rpx;
  font-weight: bold;
}
.sear_record .search_rTit .iconfont {
  font-size: 36rpx;
  float: right;
  line-height: 80rpx;
  color: #666;
}
.sear_record .search_hotBox {
  width: 100%;
  height: auto;
  overflow: hidden;
}
.sear_record .search_hotBox text {
  display: inline-block;
  font-size: 28rpx;
  line-height: 56rpx;
  background: #eee;
  padding: 0 30rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  border-radius: 40rpx;
}

