.wenzi {
  height: 75rpx;
  overflow: hidden;
}
page {
  width: 100%;
  height: 100%;
  background: #f8f8f8;
}
page .container {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0);
}
page .container .header {
  position: fixed;
  top: 0;
  width: 100%;
  height: calc(25px + 88rpx);
  background: rgba(255, 255, 255, 0);
  overflow: hidden;
  z-index: 999;
}
page .container .header.act {
  background: rgba(253, 209, 0, 0.8);
}
page .container .header.act .title-contents .titles .titleRightButton .iconfont {
  color: #fff;
}
page .container .header .title-contents {
  width: 100%;
  height: calc(25px + 88rpx);
  z-index: 100008;
}
page .container .header .title-contents .top-view {
  width: 100%;
  height: 25px;
}
page .container .header .title-contents .titles {
  display: flex;
  flex-direction: row;
  height: 88rpx;
  width: 550rpx;
}
page .container .header .title-contents .titles .input-wrapper {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 12rpx;
  margin-left: 24rpx;
  width: 702rpx;
  height: 64rpx;
  background: #fff;
  border-radius: 32rpx;
  order: 1;
}
page .container .header .title-contents .titles .input-wrapper image {
  margin: 0 15rpx 0 28rpx;
  width: 26rpx;
  height: 26rpx;
}
page .container .header .title-contents .titles .input-wrapper input {
  flex: 1;
  font-size: 28rpx;
  color: #666;
}
page .container .header .title-contents .titles .titleRightButton {
  width: 88rpx;
  height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 5px;
}
page .container .header .title-contents .titles .titleRightButton .iconfont {
  font-size: 50rpx;
  color: rgba(248, 84, 78, 0.8);
}
page .container .swiper-wrapper {
  position: relative;
  margin-top: calc(25px - 88rpx);
  width: 750rpx;
  height: 509rpx;
}
page .container .swiper-wrapper swiper {
  width: 750rpx;
  height: 509rpx;
}
page .container .swiper-wrapper swiper image {
  width: 750rpx;
  height: 509rpx;
}
page .container .time-buy {
