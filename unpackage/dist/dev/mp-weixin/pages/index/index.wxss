  width: 224rpx;
  height: auto;
}
page .container .rank .rank-list .rank-item .img-wrapper {
  width: 224rpx;
  height: 224rpx;
}
page .container .rank .rank-list .rank-item .img-wrapper image {
  width: 100%;
  height: 100%;
}
page .container .rank .rank-list .rank-item .rank-title {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 30rpx;
  margin: 22rpx 0;
}
page .container .rank .rank-list .rank-item .rank-title image {
  margin: 0 10rpx;
  width: 28rpx;
  height: 28rpx;
}
page .container .rank .rank-list .rank-item .rank-title text {
  vertical-align: middle;
  font-size: 28rpx;
  color: #111111;
}
page .container .like {
  margin: 0 auto 0;
  width: 702rpx;
}
page .container .like .title {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 90rpx;
  font-size: 32rpx;
  color: #333;
  font-weight: bold;
}
page .menu {
  position: relative;
  z-index: 99;
  width: 702rpx;
  margin: 40rpx auto 0;
  padding: 16rpx 0;
  background: #fff;
  border-radius: 12rpx;
  overflow: hidden;
}
